<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CouponDisplayView">
        <attr name="leftRedMode" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StackLabel">
        <attr name="textColor" format="color" />
        <attr name="selectTextColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="paddingVertical" format="dimension" />
        <attr name="paddingHorizontal" format="dimension" />
        <attr name="itemMargin" format="dimension" />
        <attr name="deleteButton" format="boolean" />

        <attr name="deleteButtonImage" format="reference" />
        <attr name="labelBackground" format="reference" />

        <attr name="selectMode" format="boolean" />
        <attr name="selectBackground" format="reference" />
        <attr name="maxSelectNum" format="integer" />
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="mode">
            <enum name="Default" value="0" />
            <enum name="NoLeftImage" value="1" />
            <enum name="NoLeftImage_NoDescription" value="2" />
            <enum name="NoLeftImage_NoMoreIcon" value="3" />
            <enum name="NoLeftImage_NoMoreIcon_NoDescription" value="4" />
            <enum name="NoMoreIcon" value="5" />
            <enum name="NoMoreIcon_NoDescription" value="6" />
            <enum name="NoDescription" value="7" />
        </attr>
        <attr name="smallIcon" format="boolean" />
        <attr name="image" format="reference" />
        <attr name="title" format="reference|string" />
        <attr name="subTitle" format="reference|string" />
        <attr name="description" format="reference|string" />
        <attr name="showMoreIcon" format="boolean" />
    </declare-styleable>

    <attr name="duration" format="integer" />

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomStatusView">
        <attr name="progress_width" format="dimension" />
        <attr name="progress_radius" format="dimension" />
        <attr name="loading_color" format="color" />
        <attr name="load_success_color" format="color" />
        <attr name="load_failure_color" format="color" />
    </declare-styleable>

    <!--水平滑动Tab自定义属性-->
    <!--<declare-styleable name="PullToRefreshLayout">-->
