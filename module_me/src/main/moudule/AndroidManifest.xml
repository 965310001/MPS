        <activity android:name=".ui.acitivity.order.RefundOrderInformationActivity"
            android:launchMode="singleTop" />
        <activity android:name=".ui.acitivity.order.OrderDeliveryActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".ui.acitivity.distribution.ReduceCashActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".ui.acitivity.order.EvaluateActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".ui.acitivity.order.refundorder.ApplyReturnActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".ui.acitivity.order.refundorder.ApplyRefundActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".ui.acitivity.order.refundorder.OrderRefundActivity"
            android:launchMode="singleTop"/>
